{
"entries": [
    {
    "term": "a",
    "definition": "Defines a hyperlink, linking to another page or location.",
    "syntax": "<a href=\"URL\">link text</a>",
    "example": "<a href=\"https://example.com\">Visit Example</a>",
    "source": "https://developer.mozilla.org/en-US/docs/Web/HTML/Element/a",
    "difficulty": "easy"
    },
    {
    "term": "div",
    "definition": "Generic container for flow content; used for grouping and styling elements.",
    "syntax": "<div>Content here</div>",
    "example": "<div class=\"container\">\n  <p>Hello World</p>\n</div>",
    "source": "https://developer.mozilla.org/en-US/docs/Web/HTML/Element/div",
    "difficulty": "easy"
    },
    {
    "term": "span",
    "definition": "Inline container for text or other inline elements; mainly used for styling.",
    "syntax": "<span>Inline content</span>",
    "example": "<p>This is <span class=\"highlight\">highlighted</span> text.</p>",
    "source": "https://developer.mozilla.org/en-US/docs/Web/HTML/Element/span",
    "difficulty": "easy"
    },
    {
    "term": "img",
    "definition": "Embeds an image into the document.",
    "syntax": "<img src=\"URL\" alt=\"description\">",
    "example": "<img src=\"image.jpg\" alt=\"A descriptive text\">",
    "source": "https://developer.mozilla.org/en-US/docs/Web/HTML/Element/img",
    "difficulty": "easy"
    },
    {
    "term": "h1",
    "definition": "Defines the highest-level heading.",
    "syntax": "<h1>Heading Text</h1>",
    "example": "<h1>Welcome to My Site</h1>",
    "source": "https://developer.mozilla.org/en-US/docs/Web/HTML/Element/Heading_Elements",
    "difficulty": "easy"
    },
    {
    "term": "ul",
    "definition": "Defines an unordered list.",
    "syntax": "<ul><li>Item</li></ul>",
    "example": "<ul>\n  <li>First</li>\n  <li>Second</li>\n</ul>",
    "source": "https://developer.mozilla.org/en-US/docs/Web/HTML/Element/ul",
    "difficulty": "easy"
    },
    {
    "term": "ol",
    "definition": "Defines an ordered list.",
    "syntax": "<ol><li>Item</li></ol>",
    "example": "<ol>\n  <li>First</li>\n  <li>Second</li>\n</ol>",
    "source": "https://developer.mozilla.org/en-US/docs/Web/HTML/Element/ol",
    "difficulty": "easy"
    },
    {
    "term": "li",
    "definition": "Defines a list item, used inside ul, ol, or menu.",
    "syntax": "<li>Item content</li>",
    "example": "<ul>\n  <li>Item 1</li>\n  <li>Item 2</li>\n</ul>",
    "source": "https://developer.mozilla.org/en-US/docs/Web/HTML/Element/li",
    "difficulty": "easy"
    },
    {
    "term": "form",
    "definition": "Represents a section of a document that contains interactive controls for submitting data.",
    "syntax": "<form action=\"URL\" method=\"GET|POST\">...</form>",
    "example": "<form action=\"submit.php\" method=\"post\">\n  <input type=\"text\" name=\"username\">\n  <button type=\"submit\">Send</button>\n</form>",
    "source": "https://developer.mozilla.org/en-US/docs/Web/HTML/Element/form",
    "difficulty": "medium"
    },
    {
    "term": "input",
    "definition": "Defines a control for user input.",
    "syntax": "<input type=\"text\" name=\"name\">",
    "example": "<input type=\"email\" name=\"email\" placeholder=\"Enter your email\">",
    "source": "https://developer.mozilla.org/en-US/docs/Web/HTML/Element/input",
    "difficulty": "medium"
    }
]
}
